{
    "name": "parsa-mirzaie/docker-starter-kit-laravel",
    "description": "A Docker starter kit for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Parsa Mirzaie",
            "email": "Mirzaie_parsa@protonmail.ch"
        }
    ],
    "keywords": ["laravel", "docker", "nginx", "php-fpm", "mysql", "starter-kit"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "ParsaMirzaie\\DockerStarterKitLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ParsaMirzaie\\DockerStarterKitLaravel\\DockerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}